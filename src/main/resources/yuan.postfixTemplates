# object

.newvar : new object
	ANY                      →  $expr$ $END$ = new $expr$();

.ifnull : if object is null
	ANY                      →  if($expr$ == null){\
		$END$\
	}

.ifnotnull : if object is not null
	ANY                      →  if($expr$ != null){\
		$END$\
	}

.if : if object
	ANY                      →  if($expr$$END$){\
		\
	}